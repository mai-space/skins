{
  "name": "rubb1/skins",
  "type": "typo3-cms-extension",
  "description": "This extension adds a dark skin (dark theme) to the TYPO3 backend. The colors are customizable and can be changed to your favorite colors.",
  "version": "13",
  "keywords": [
    "TYPO3",
    "extension",
    "backend",
    "skin",
    "dark",
    "mode",
    "darkmode",
    "custom",
    "save-and-close"
  ],
  "homepage": "https://www.myc3.com/",
  "authors": [
    {
      "name": "Dominik Schüßler",
      "role": "Developer",
      "homepage": "https://www.myc3.com/"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Rubb1\\Skins\\": "Classes/"
    }
  },
  "require": {
    "php": "^8.2",
    "typo3/cms-core": "^13.1"
  },
  "extra": {
    "tags": "Stable, PHP 8.2, TYPO3 13.1, English, OpenSource, Free, TER",
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "extension-key": "skins"
    }
  }
}
